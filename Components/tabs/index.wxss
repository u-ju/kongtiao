.tabsh{
  width: 100%;
  height: 88rpx;
}
.wux-tabs {
  display: -ms-flexbox;
  
  display: flex;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  -ms-flex-pack: distribute;
  justify-content: space-around;
  -ms-flex-align: center;
  align-items: center;
  background: #fff;
  z-index: 999999;
}
.wux-tabs--scroll {
  display: block;
  overflow: auto;
  white-space: nowrap
}
.wux-tabs--vertical {
  display: -ms-inline-flexbox;
  display: inline-flex;
  width: auto;
  height: auto;
  max-height: 540rpx;
  -ms-flex-direction: column;
  flex-direction: column
}